<div class="container mt-5">
    <div class="row">
        
        
        
        <div class="col-lg-8 offset-lg-2">
            <div class="card">
                <div class="card-body text-center">
                    <span><h1>LISTADO DE PRODUCTOS</h1></span>
                    <a href="/notes/agregarProductos"> Nuevooo</a>
                    
                    <table class="table table-striped mt-4">
                        
  <thead>
      
    <tr>
      <th scope="col">Producto</th>
      <th scope="col">Categoria</th>
      <th scope="col">Ubicacion</th>
      <th scope="col">Precio</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
      {{#each productos}}
    <tr>
      <th>{{producto}}</th>
      <td>{{categoria}}</td>
      <td>{{ubicacion}}</td>
      <td>{{precio}}</td>

      <td><i class="fa-solid fa-trash text-danger"></i>
      <i class="fa-solid fa-pen-to-square text-primary"></i></td>
    </tr>
    
    {{/each}}
  </tbody>
  
</table>
                </div>
            </div>
        </div>
        
    </div>
</div>